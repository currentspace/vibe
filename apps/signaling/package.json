{
  "name": "signaling",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test vitest",
    "test:watch": "NODE_ENV=test vitest --watch",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts --out ../../docs/api/signaling",
    "docs:serve": "echo 'API docs available at http://localhost:4000/api-docs when running the server'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^22.10.6",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/ui": "^3.2.1",
    "eslint": "^9.17.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.1",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.19.2",
    "typedoc": "^0.28.5",
    "typescript": "^5.7.3",
    "vitest": "^3.2.1"
  }
}