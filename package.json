{
  "name": "vibe",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=23.0.0"
  },
  "scripts": {
    "dev": "pnpm --parallel --filter=web --filter=signaling run dev",
    "dev:web": "pnpm --filter web dev",
    "dev:signaling": "pnpm --filter signaling dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {}
}
